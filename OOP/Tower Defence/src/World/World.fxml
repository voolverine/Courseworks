<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.control.TableRow?>

<?import javafx.scene.control.Label?>
<Pane fx:id="pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="World.World">
   <children>
   
       <Canvas fx:id="canvas" />
       <GridPane fx:id="score_panel">
            <columnConstraints>
                <ColumnConstraints percentWidth="33.3"/>
                <ColumnConstraints percentWidth="33.3"/>
                <ColumnConstraints percentWidth="33.3"/>
            </columnConstraints>
           <StackPane GridPane.rowIndex="0" GridPane.columnIndex="0" fx:id="Credits">
               <Label text="Credits"/>
           </StackPane>
           <StackPane GridPane.rowIndex="0" GridPane.columnIndex="1" fx:id="time">
                <Label text="Time" />
           </StackPane>
           <StackPane GridPane.rowIndex="0" GridPane.columnIndex="2" fx:id="progress_bar">
                <Label text="Progress" />
           </StackPane>

       </GridPane>

       <ImageView fx:id="imageView" />
       <Rectangle fx:id="map_focus" />
   </children>

</Pane>
